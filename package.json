{
  "name": "karolis.sh",
  "private": true,
  "description": "Source code of karolis.sh",
  "repository": {
    "type": "git",
    "url": "git@github.com:karolis-sh/karolis.sh.git"
  },
  "license": "MIT",
  "author": "Karolis Å arapnickis",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public .cache",
    "deploy": "gatsby-plugin-s3 deploy -y",
    "dev": "gatsby develop",
    "format": "run-p format:*",
    "format:js": "prettier --write '**/*.*'",
    "format:package-json": "sort-package-json",
    "lint": "run-p lint:*",
    "lint:js": "eslint .",
    "lint:md": "markdownlint-cli2 '**/*.md' '!node_modules' '!**/node_modules'",
    "qa": "yarn lint",
    "release": "yarn && run-s clean build deploy",
    "try": "run-s clean build && gatsby serve -p 8000",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/package.json": "sort-package-json",
    "*.js": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@mdx-js/react": "^1.6.22",
    "classnames": "^2.3.1",
    "gatsby": "^3.7.1",
    "gatsby-plugin-google-analytics": "^3.7.1",
    "gatsby-plugin-next-seo": "^1.7.0",
    "prism-react-renderer": "^1.2.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.0.9"
  },
  "devDependencies": {
    "@k.sh/eslint-config": "^1.21.0",
    "@mdx-js/mdx": "^1.6.22",
    "autoprefixer": "^10.2.6",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "cssnano": "^5.0.6",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^10.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-prettier": "^3.4.0",
    "gatsby-plugin-advanced-sitemap": "^2.0.0",
    "gatsby-plugin-catch-links": "^3.7.1",
    "gatsby-plugin-force-trailing-slashes": "^1.0.5",
    "gatsby-plugin-manifest": "^3.7.1",
    "gatsby-plugin-mdx": "^2.7.1",
    "gatsby-plugin-no-sourcemaps": "^3.7.0",
    "gatsby-plugin-postcss": "^4.7.1",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-robots-txt": "^1.6.2",
    "gatsby-plugin-s3": "^0.3.8",
    "gatsby-plugin-sharp": "^3.7.1",
    "gatsby-remark-autolink-headers": "^4.4.1",
    "gatsby-remark-images": "^5.4.1",
    "gatsby-source-filesystem": "^3.7.1",
    "husky": "^6.0.0",
    "levenary": "^1.1.1",
    "lint-staged": ">=10",
    "markdownlint-cli2": "^0.1.3",
    "npm-run-all": "^4.1.3",
    "pretty-quick": "^3.1.0",
    "remark-external-links": "^8.0.0",
    "rimraf": "^3.0.2",
    "sort-package-json": "^1.50.0",
    "tailwindcss": "^2.1.4"
  }
}
