{
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:karolis-sh/karolis.sh.git"
  },
  "license": "MIT",
  "author": "Karolis Å arapnickis",
  "scripts": {
    "build": "gatsby build",
    "check-deps": "npm ls >/dev/null",
    "clean": "rimraf public .cache",
    "format": "run-p format:*",
    "format:js": "prettier --write '**/*.*'",
    "format:package-json": "sort-package-json",
    "lint": "eslint src",
    "prepush": "npm run qa",
    "qa": "run-p lint",
    "start": "gatsby develop",
    "try": "rm -rf public && npm run build && gatsby serve",
    "upgrade-interactive": "npm-check --update"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "prod_url": "https://karolis.sh/"
  },
  "dependencies": {
    "babel-plugin-emotion": "^9.2.5",
    "emotion": "^9.2.5",
    "emotion-server": "^9.2.5",
    "gatsby": "^1.9.273",
    "gatsby-image": "^1.0.54",
    "gatsby-plugin-emotion": "^1.1.17",
    "gatsby-plugin-google-analytics": "^1.0.31",
    "gatsby-plugin-offline": "^1.0.18",
    "gatsby-plugin-preact": "^1.0.17",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "gatsby-plugin-robots-txt": "^1.1.0",
    "gatsby-plugin-sitemap": "^1.2.25",
    "npm-check": "^5.9.0",
    "react-emotion": "^9.2.5",
    "react-helmet": "^5.2.0",
    "react-inlinesvg": "^0.8.1"
  },
  "devDependencies": {
    "@k.sh/eslint-config": "^1.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.10.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "sort-package-json": "^1.15.0"
  }
}
